{
  "$id": "http://example.com/example.json",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "name": {
      "$id": "/properties/name",
      "type": "string",
      "title": "The Name Schema ",
      "default": "",
      "minLength": 1,
      "maxLength": 55,
      "examples": ["Rithm Inc"]
    },
    "handle": {
      "$id": "/properties/handle",
      "type": "string",
      "title": "The Handle Schema ",
      "default": "",
      "minLength": 1,
      "maxLength": 55,
      "examples": ["rithm"]
    },
    "logo": {
      "$id": "/properties/logo",
      "type": "string",
      "title": "The Logo Schema ",
      "default": "",
      "format": "uri",
      "examples": [
        "https://ih1.redbubble.net/image.396326354.4958/flat,800x800,075,t.u1.jpg"
      ]
    },
    "_token": {
      "$id": "/properties/token",
      "type": "string",
      "title": "The Token Schema ",
      "default": "",
      "examples": [
        "foo123"
      ]
    }
  },
  "required": ["name", "handle"]
}
