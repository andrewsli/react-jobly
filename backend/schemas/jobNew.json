{
  "$id": "http://example.com/example.json",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "title": {
      "$id": "/properties/title",
      "type": "string",
      "title": "The Title Schema ",
      "default": "",
      "minLength": 1,
      "maxLength": 55,
      "examples": ["Lead Instructor"]
    },
    "company_handle": {
      "$id": "/properties/company_handle",
      "type": "string",
      "title": "The Handle Schema ",
      "default": "",
      "minLength": 1,
      "maxLength": 55,
      "examples": ["rithm"]
    },
    "salary": {
      "$id": "/properties/salary",
      "type": "number",
      "title": "The Salary Schema ",
      "default": "",
      "examples": [1000000000]
    },
    "equity": {
      "$id": "/properties/equity",
      "type": "number",
      "title": "The Equity Schema ",
      "default": 0,
      "examples": [0.5]
    },
    "_token": {
      "$id": "/properties/token",
      "type": "string",
      "title": "The Token Schema ",
      "default": "",
      "examples": [
        "foo123"
      ]
    }
  },
  "required": ["title", "salary"]
}
